  "require": {
    "php": ">=8.1",
    "laravel/framework": ">=8",
    "illuminate/support": ">=8",
    "laravel/helpers": "^1.5"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "mPhpMaster",
      "email": "59211285+mPhpMaster@users.noreply.github.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "MPhpMaster\\ModelQuerySelector\\": "src/"
    },
    "files": [
      "src/Helpers/FHelpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MPhpMaster\\ModelQuerySelector\\ModelQuerySelectorServiceProvider"
      ]
